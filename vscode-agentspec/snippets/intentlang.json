{
  "Package Declaration": {
    "prefix": "package",
    "description": "Package declaration with version and lang",
    "body": [
      "package \"${1:my-package}\" version \"${2:1.0.0}\" lang \"3.0\"",
      ""
    ]
  },
  "Import": {
    "prefix": "import",
    "description": "Import a local .ias file or package",
    "body": [
      "import \"${1:./module.ias}\" as ${2:mod}"
    ]
  },
  "Agent Block": {
    "prefix": "agent",
    "description": "Define an agent",
    "body": [
      "agent \"${1:agent-name}\" {",
      "  model \"${2|ollama/llama3.2,claude-sonnet-4-20250514,openai/gpt-4o|}\"",
      "  prompt \"${3:system-prompt}\"",
      "  strategy \"${4|react,plan_execute,reflexion,router,map_reduce|}\"",
      "  max_turns ${5:10}",
      "}"
    ]
  },
  "Agent with Config": {
    "prefix": "agent-config",
    "description": "Agent with config, validation, and eval blocks",
    "body": [
      "agent \"${1:agent-name}\" {",
      "  model \"${2|ollama/llama3.2,claude-sonnet-4-20250514,openai/gpt-4o|}\"",
      "  prompt \"${3:system-prompt}\"",
      "  strategy \"react\"",
      "  max_turns ${4:10}",
      "",
      "  uses skill \"${5:skill-name}\"",
      "",
      "  config {",
      "    ${6:param_name} string default \"${7:value}\" \"${8:Description}\"",
      "  }",
      "",
      "  validate {",
      "    rule ${9:rule_name} ${10|warning,error|} \"${11:Rule description}\" when output != \"\"",
      "  }",
      "",
      "  eval {",
      "    case ${12:test_name} input \"${13:Hello}\" expect \"${14:hello}\" scoring contains",
      "  }",
      "}"
    ]
  },
  "Ollama Agent": {
    "prefix": "agent-ollama",
    "description": "Agent using local Ollama model",
    "body": [
      "agent \"${1:agent-name}\" {",
      "  model \"ollama/${2|llama3.2,mistral,codellama:7b,qwen2.5-coder:7b,deepseek-coder:6.7b|}\"",
      "  prompt \"${3:system-prompt}\"",
      "  strategy \"react\"",
      "  max_turns ${4:10}",
      "  stream true",
      "}"
    ]
  },
  "Prompt Block": {
    "prefix": "prompt",
    "description": "Define a prompt",
    "body": [
      "prompt \"${1:prompt-name}\" {",
      "  content \"${2:You are a helpful assistant.}\"",
      "}"
    ]
  },
  "Skill Block": {
    "prefix": "skill",
    "description": "Define a skill with input/output and tool",
    "body": [
      "skill \"${1:skill-name}\" {",
      "  description \"${2:What this skill does}\"",
      "",
      "  input {",
      "    ${3:param} string required",
      "  }",
      "",
      "  output {",
      "    ${4:result} string required",
      "  }",
      "",
      "  tool ${5|http,command,inline,mcp|} {",
      "    ${6:method \"POST\"}",
      "    ${7:url \"https://api.example.com\"}",
      "  }",
      "}"
    ]
  },
  "Skill Inline": {
    "prefix": "skill-inline",
    "description": "Skill with inline code tool",
    "body": [
      "skill \"${1:skill-name}\" {",
      "  description \"${2:What this skill does}\"",
      "",
      "  input {",
      "    ${3:param} string required",
      "  }",
      "",
      "  output {",
      "    ${4:result} string required",
      "  }",
      "",
      "  tool inline {",
      "    language \"${5|bash,python|}\"",
      "    code \"${6:echo hello}\"",
      "  }",
      "}"
    ]
  },
  "Config Block": {
    "prefix": "config",
    "description": "Config block inside an agent",
    "body": [
      "config {",
      "  ${1:param_name} ${2|string,int,float,bool|} default \"${3:value}\" \"${4:Description}\"",
      "  ${5:api_key} string required secret \"${6:API key}\"",
      "}"
    ]
  },
  "Validate Block": {
    "prefix": "validate",
    "description": "Validation rules block inside an agent",
    "body": [
      "validate {",
      "  rule ${1:rule_name} ${2|warning,error|} \"${3:Description}\" when ${4:output != \"\"}",
      "}"
    ]
  },
  "Eval Block": {
    "prefix": "eval",
    "description": "Evaluation test cases block inside an agent",
    "body": [
      "eval {",
      "  case ${1:test_name} input \"${2:test input}\" expect \"${3:expected}\" scoring ${4|contains,semantic,exact,regex|}",
      "}"
    ]
  },
  "On Input Block": {
    "prefix": "on-input",
    "description": "On input control flow block",
    "body": [
      "on input {",
      "  if ${1:input == \"hello\"} {",
      "    ${2:use skill \"skill-name\"}",
      "  } else {",
      "    ${3:respond \"I received your message.\"}",
      "  }",
      "}"
    ]
  },
  "If Else": {
    "prefix": "if",
    "description": "If/else conditional block",
    "body": [
      "if ${1:condition} {",
      "  ${2:use skill \"skill-name\"}",
      "} else {",
      "  ${3:respond \"fallback\"}",
      "}"
    ]
  },
  "For Each": {
    "prefix": "foreach",
    "description": "For each loop",
    "body": [
      "for each ${1:item} in ${2:items} {",
      "  ${3:use skill \"skill-name\"}",
      "}"
    ]
  },
  "Rule": {
    "prefix": "rule",
    "description": "Validation rule",
    "body": [
      "rule ${1:rule_name} ${2|warning,error|} \"${3:Rule description}\" when ${4:output != \"\"}"
    ]
  },
  "Case": {
    "prefix": "case",
    "description": "Evaluation test case",
    "body": [
      "case ${1:test_name} input \"${2:test input}\" expect \"${3:expected}\" scoring ${4|contains,semantic,exact,regex|} threshold ${5:0.8}"
    ]
  },
  "Deploy Block": {
    "prefix": "deploy",
    "description": "Deployment target block",
    "body": [
      "deploy \"${1:local}\" target \"${2|process,docker,kubernetes,compose|}\" {",
      "  port ${3:8080}",
      "  health {",
      "    path \"/healthz\"",
      "  }",
      "}"
    ]
  },
  "Pipeline Block": {
    "prefix": "pipeline",
    "description": "Multi-agent pipeline",
    "body": [
      "pipeline \"${1:pipeline-name}\" {",
      "  step \"${2:step-1}\" {",
      "    agent \"${3:agent-name}\"",
      "  }",
      "  step \"${4:step-2}\" {",
      "    agent \"${5:agent-name}\"",
      "    depends_on [\"${2:step-1}\"]",
      "  }",
      "}"
    ]
  },
  "MCP Tool": {
    "prefix": "tool-mcp",
    "description": "MCP tool configuration",
    "body": [
      "tool mcp {",
      "  server \"${1:server-name}\"",
      "}"
    ]
  },
  "HTTP Tool": {
    "prefix": "tool-http",
    "description": "HTTP tool configuration",
    "body": [
      "tool http {",
      "  method \"${1|GET,POST,PUT,DELETE|}\"",
      "  url \"${2:https://api.example.com}\"",
      "}"
    ]
  },
  "Command Tool": {
    "prefix": "tool-command",
    "description": "Command tool configuration",
    "body": [
      "tool command {",
      "  binary \"${1:command}\"",
      "  args \"${2:arg1}\"",
      "}"
    ]
  },
  "Inline Tool": {
    "prefix": "tool-inline",
    "description": "Inline code tool configuration",
    "body": [
      "tool inline {",
      "  language \"${1|bash,python|}\"",
      "  code \"${2:echo hello}\"",
      "}"
    ]
  },
  "Memory Block": {
    "prefix": "memory",
    "description": "Memory configuration",
    "body": [
      "memory {",
      "  strategy \"${1|sliding_window,summary|}\"",
      "  max_messages ${2:20}",
      "}"
    ]
  }
}
